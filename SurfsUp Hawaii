CREATE Table hawaii_stations (
	station VARCHAR (20) PRIMARY KEY NOT NULL,
	name VARCHAR (50) NOT NULL,
	latitude INTEGER (10) NOT NULL,
	longitude INTEGER (10) NOT NULL,
	elevation INTEGER (10) NOT NULL,
	);
	
